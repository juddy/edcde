<Sect1 Id="CalendarFileMenu">
<Title>Calendar File Menu</Title>
<IndexTerm>
<Primary SortAs="file menu">File menu</Primary>
</IndexTerm>
<!--	<idx>File menu:Print Current View<sort>file menu:print current view<\idx> -->
<IndexTerm>
