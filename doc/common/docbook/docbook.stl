<!-- DTINFO StyleSheet for the CDE and Motif docs (as DocBook instances).  -->

<STYLESHEET Name="cdedoc">

<AutoNumber
	id=stepnum
	counter=Step
	reset="Procedure SubSteps"
>
<AutoNumber
	id=listnum
	counter=ListItem
	reset="OrderedList ItemizedList"
>
<AutoNumber
	id=listnumua
	counter=ListItem
	reset="OrderedList ItemizedList"
	type=ucalpha
	initial="A"
>
<AutoNumber
	id=listnumla
	counter=ListItem
	reset="OrderedList ItemizedList"
	type=lcalpha
	initial="a"
>
<AutoNumber
	id=listnumur
	counter=ListItem
	reset="OrderedList ItemizedList"
	type=ucroman
	initial="I"
>
<AutoNumber
	id=listnumlr
	counter=ListItem
	reset="OrderedList ItemizedList"
	type=lcroman
	initial="i"
>
<AutoNumber
	id=figurenum
	counter=Figure
	reset="Preface Chapter Appendix"
>
<AutoNumber
	id=tablenum
	counter=Table
	reset="Preface Chapter Appendix"
>

